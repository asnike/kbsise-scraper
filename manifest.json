{
  "manifest_version":2,
  "name":"kbsise-scraper",
  "description":"kbsise scraper",
  "version":"1.2",
  "icons":{
    "48":"icon.png"
  },
  "browser_action":{
      "default_icon":"icon.png",
      "default_popup":"popup.html"
  },
  "permissions":[
      "<all_urls>",
      "notifications",
      "tabs",
      "http://*/",
      "https://*/"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
